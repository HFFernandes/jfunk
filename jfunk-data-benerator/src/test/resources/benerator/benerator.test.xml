	<create-entities name="ustva" count="1" consumer="com.mgmtp.jfunk.data.benerator.BeneratorDataSource$DataSetConsumer">

		<!-- Data for finanzaemter (Land, Amt, Nummer) from csv-file -->
		<variable name="finanzaemter" type="entity" source="src/test/resources/benerator/finanzaemter.csv" cyclic="true"/>

		<!-- Data for mutually exclusive radio button fields -->
		<variable name="manu_auto" type="entity" source="src/test/resources/benerator/manu_auto.csv" cyclic="true"/>

		<!-- Data for various persons and tax-ids by benerator standard Generators -->
		<variable name="person" generator="org.databene.domain.person.PersonGenerator" dataset="DE" locale="de"/>

		<variable name="firma" generator="org.databene.domain.person.PersonGenerator" dataset="DE" locale="de"/>

		<variable name="mitwirk" generator="org.databene.domain.person.PersonGenerator" dataset="DE" locale="de"/>

		<variable name="taxid" generator="org.databene.domain.person.TaxIdGenerator_DE" dataset="DE" locale="de"/>

		<!-- Definitions of attributes to be later on used by the consumer (order irrelevant)-->

		<attribute name="radioSteuernummer" source="manu_auto.Manuell"/>
		<attribute name="steuernummerProfil" source="taxid"/>
		<attribute name="bundesland" source="finanzaemter.Land"/>
		<attribute name="finanzamt" source="finanzaemter.Amt"/>
		<attribute name="finanzamtsNummer" source="finanzaemter.Nummer"/>
		<attribute name="steuernummerBezirk" pattern="[0-9]{3,4}"/>
		<attribute name="steuernummerUnterscheidungsnummer" pattern="[0-9]{4,5}"/>
		<attribute name="anzeigesteuernummer" source="taxid"/>
		<attribute name="notiz" pattern=".{0,200}"/>
		<attribute name="jahr" pattern="[2-2]{1,1}[0-0]{1,1}[1-9]{1,1}[0-9]{1,1}"/>
		<attribute name="voranmeldungszeitraum" pattern="[1-5]{1}"/>
		<attribute name="kz10" source="manu_auto.Manuell"/>
		<attribute name="kz22" source="manu_auto.Manuell"/>
		<attribute name="nameOderUnternehmen" source="firma.familyName"/>
		<attribute name="radioMitwirkungKeine" source="manu_auto.Manuell"/>
		<attribute name="radioMitwirkungKanzlei" source="manu_auto.Manuell"/>
		<attribute name="radioMitwirkungBearbeiter" source="manu_auto.Manuell"/>
